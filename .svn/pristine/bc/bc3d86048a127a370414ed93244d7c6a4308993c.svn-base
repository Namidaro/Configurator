        </Grid.ColumnDefinitions>
        <StackPanel Grid.Row="1">
            <Label Content="Дата:" />
            <Label Content="Время:" />
            <Label Content="Серийный номер:" />
            <Label Content="Ua(В):" />
            <Label Content="Ub(В):" />
            <Label Content="Uc(В):" />
            <Label Content="Ia(A):" />
            <Label Content="Ib(A):" />
            <Label Content="Ic(A):" />
            <Label Content="Pa(кВт):" />
