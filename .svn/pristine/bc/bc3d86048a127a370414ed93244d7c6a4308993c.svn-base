            <Label x:Name="uiCurrentC" Content="****" />
            <Label x:Name="uiPowerA" Content="****" />
            <Label x:Name="uiPowerB" Content="****" />
            <Label x:Name="uiPowerC" Content="****" />
            <Label x:Name="uiEnergyO" Content="****" />
            <Label x:Name="uiEnergyM" Content="****" />
            <Label x:Name="uiEnergyD" Content="****" />

        </StackPanel>
    </Grid>
</UserControl>
